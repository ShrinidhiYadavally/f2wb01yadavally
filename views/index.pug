extends layout

block content
  h1= title
  p Welcome to Hyderabad  #{title}
  button(type="button", class="btn btn-secondary", onclick="doBump()") Press Me for the contents
  br
  br
  div(id="bump") Bump the count
  script. 
    var count=0;
    var bump=1
    function doBump(){
      count = count+bump;
      bump =bump+1;
      document.getElementById("bump").innerHTML="Count " + count +" with bump "+bump  ;
    }
br
  br
  a(href="kite") Click here for kites Page   